{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44454 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44454 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "ng build && gzipper compress ./dist",
    "compress": "gzipper compress ./dist",
    "build:ssr": "ng run Web:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^16.1.6",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/platform-server": "^15.0.0",
    "@angular/router": "^15.0.0",
    "audit": "^0.0.6",
    "bootstrap": "^5.3.0",
    "compression": "^1.7.4",
    "fix": "^0.0.6",
    "jquery": "^3.7.0",
    "oidc-client": "^1.11.5",
    "run-script-os": "^1.1.6",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.3",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.0.0",
    "@popperjs/core": "^2.11.8",
    "@types/jasmine": "^4.3.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.11.18",
    "gzipper": "^7.2.0",
    "jasmine-core": "~4.5.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~4.8.2"
  }
}
